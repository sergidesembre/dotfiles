# List (basic, all, directories)
alias ll='ls -lh'
alias la='ls -lah'
alias lsd='ls -lh | grep --color=never "^d"'

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

# Jumps
alias work='cd ~/Code/work'
alias me='cd ~/Code/me'
alias dotfiles='cd ~/.dotfiles'

# Miscellaneous
alias iplocal='hostname -I'
alias install='sudo apt install'
alias remove='sudo apt remove --purge'
